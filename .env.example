# API Keys
JOBSFORHER_API_KEY=your_api_key_here
OPENAI_API_KEY=your_openai_api_key_here

# Security
JWT_SECRET_KEY=generate_secure_key_here
ENCRYPTION_KEY=generate_fernet_key_here

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/asha_ai

# Monitoring
ENABLE_ANALYTICS=true
LOG_LEVEL=INFO

# External Services
EVENTS_API_URL=https://api.events.example.com
MENTORSHIP_API_URL=https://api.mentorship.example.com

# Rate Limiting
MAX_REQUESTS_PER_MINUTE=60
RATE_LIMIT_ENABLED=true

# Feature Flags
ENABLE_BIAS_DETECTION=true
ENABLE_REAL_TIME_UPDATES=true
CACHE_ENABLED=true 